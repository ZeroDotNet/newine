			<label class="col-sm-2">Nombre o Apellido</label>
			<div class="col-sm-10">
				<input type="text" class="form-control" name="q[user_name_or_user_surname_start]" value="<%= @q.user_name_or_user_surname_start %>" />
			</div>
		</div>
		<div class="form-group">
			<div class="col-sm-12">
				<div class="pull-right">
